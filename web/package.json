{
  "name": "react-web-demo",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "antd": "^3.19.1",
    "axios": "^0.19.0",
    "chalk": "1.1.3",
    "dotenv": "4.0.0",
    "formik": "^1.4.1",
    "fs-extra": "3.0.1",
    "lodash": "^4.17.11",
    "mobx": "^5.0.3",
    "mobx-react": "^5.2.5",
    "object-assign": "4.1.1",
    "promise": "8.0.1",
    "query-string": "^6.3.0",
    "raf": "3.4.0",
    "react-digraph": "^6.2.2",
    "react-syntax-highlighter": "^10.3.0",
    "recharts": "^1.6.2",
    "redoc": "^2.0.0-alpha.41",
    "yaml-loader": "0.5.0"
  },
  "scripts": {
    "precommit": "lint-staged",
    "lint": "eslint src",
    "start": "REACT_APP_API_BASE_URL=/apis/v1alpha1 node scripts/start.js",
    "build": "REACT_APP_API_BASE_URL=/apis/v1alpha1 node scripts/build.js",
    "test": "node scripts/test.js --env=jsdom"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "mjs",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "lint-staged": {
    "*.{js,json,css,less}": [
      "prettier --write",
      "git add"
    ],
    "src/**/*.js": [
      "eslint --max-warnings=0"
    ]
  },
  "babel": {
    "plugins": [
      "transform-decorators-legacy"
    ],
    "presets": [
      "react-app"
    ]
  },
  "devDependencies": {
    "autoprefixer": "7.1.6",
    "babel-core": "6.26.0",
    "babel-eslint": "8.2.1",
    "babel-jest": "^24.8.0",
    "babel-loader": "7.1.2",
    "babel-plugin-import": "^1.6.3",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-react-app": "^3.1.0",
    "babel-runtime": "6.26.0",
    "case-sensitive-paths-webpack-plugin": "2.1.2",
    "classnames": "^2.2.6",
    "css-loader": "2.1.0",
    "eslint": "4.18.2",
    "eslint-config-prettier": "^3.3.0",
    "eslint-config-react-app": "^2.0.1",
    "eslint-loader": "2.1.1",
    "eslint-plugin-flowtype": "2.41.0",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-react": "7.4.0",
    "file-loader": "3.0.1",
    "handlebars": "^4.1.2",
    "html-webpack-plugin": "^4.0.0-beta.4",
    "http-proxy-middleware": "^0.19.1",
    "husky": "^1.3.0",
    "jest": "20.0.4",
    "json-loader": "^0.5.7",
    "less": "^2.7.3",
    "less-loader": "^4.1.0",
    "lint-staged": "^8.1.0",
    "mini-css-extract-plugin": "0.5.0",
    "moment": "^2.24.0",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "3.0.0",
    "prettier": "^1.15.3",
    "prop-types": "^15.6.2",
    "quantity.js": "^1.1.3",
    "react": "^16.2.0",
    "react-dev-utils": "^6.0.0",
    "react-dom": "^16.2.0",
    "react-intl-universal": "^1.15.4",
    "react-router-dom": "^4.2.2",
    "style-loader": "0.23.1",
    "styled-components": "^4.1.3",
    "sw-precache-webpack-plugin": "0.11.5",
    "terser-webpack-plugin": "^1.2.3",
    "url-loader": "1.1.2",
    "webpack": "4.28.4",
    "webpack-dev-server": "3.2.1",
    "webpack-manifest-plugin": "2.0.4"
  },
  "peerDependencies": {},
  "proxy": "http://192.168.21.107:30022"
}
